{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "OFSEC Docs",
  "colors": {
    "primary": "#0f172a",
    "light": "#334155",
    "dark": "#020617"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Başlangıç",
            "pages": [
              "index",
              "quickstart",
              "setup"
            ]
          },
          {
            "group": "Güvenlik Araştırmaları",
            "pages": [
              "research/web",
              "research/mobile",
              "research/crypto"
            ]
          },
          {
            "group": "Araçlar & Metodolojiler",
            "pages": [
              "tools/burp",
              "tools/gdb",
              "tools/foundry",
              "tools/reversing"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api/introduction"
            ]
          },
          {
            "group": "Examples",
            "pages": [
              "api/examples/get",
              "api/examples/post",
              "api/examples/delete"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Ana Site",
          "href": "https://visec.net.tr",
          "icon": "home"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/Enay-Project",
          "icon": "github"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Destek",
        "href": "mailto:info@visec.net.tr"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://ofsec.visec.net.tr"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/enayproject",
      "github": "https://github.com/enayproject",
      "linkedin": "https://linkedin.com/in/enay"
    }
  }
}
